    "description": "Accuvend Application for power vending",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "start": " nodemon -r newrelic ./src/app.ts",
        "dev": "nodemon -r newrelic ./src/app.ts",
        "debug": "ts-node-dev --inspect=4321 --respawn --transpile-only --project tsconfig.json ./src/app.ts",
        "dev:render": "node ./dist/app.js",
        "build": "tsc --project tsconfig.json",
        "lint": "tslint -c tslint.json -p tsconfig.json --fix",
        "db:makemigration": "sequelize db:migrate --config ./config/config.json --env production",
        "db:undomigration": "sequelize db:migrate:undo --env production --config ./config/config.json"
    },
